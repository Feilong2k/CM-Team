# Backend env
PORT=3500
# Allow localhost:6100-6120 (frontend port range)
CORS_ORIGIN_REGEX=^http:\/\/localhost:61[0-1][0-9]$

# LOCAL POSTGRES
DATABASE_URL=postgresql://username:password@localhost:5432/dbname

# Tracing / debug
# Trace timeline (in-memory) is ON by default.
# Set TRACE_ENABLED=false to disable /api/trace and stop logging.
TRACE_ENABLED=true

# Duplication probe files are OFF by default.
ORION_DUP_PROBE_ENABLED=false
# ORION_DUP_PROBE_DIR=backend/debug/dup_probe
